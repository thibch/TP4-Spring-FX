            <HBox alignment="CENTER" prefHeight="41.0" prefWidth="600.0" spacing="60.0">
                <children>
                    <Label text="Niveau" />
                    <TextField fx:id="priceField" prefWidth="200.0" />
                </children></HBox>
            <HBox alignment="CENTER" prefHeight="39.0" prefWidth="360.0">
                <children>
                    <Button fx:id="validateButton" mnemonicParsing="false" onAction="#validateButton" prefWidth="125.0" text="Valider" />
