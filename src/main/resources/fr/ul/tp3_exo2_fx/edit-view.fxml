<VBox alignment="TOP_CENTER" prefHeight="501.0" prefWidth="336.0" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.ul.tp3_exo2_fx.controller.EditView">
<children>
